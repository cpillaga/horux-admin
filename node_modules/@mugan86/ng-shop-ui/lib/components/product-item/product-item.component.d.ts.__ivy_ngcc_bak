import { IProduct } from './../../interfaces/product.interface';
import { CURRENCIES_SYMBOL } from './../../constants/currencies.enum';
import { EventEmitter, OnInit } from '@angular/core';
export declare class ProductItemComponent implements OnInit {
    product: IProduct;
    showDesc: boolean;
    add: EventEmitter<IProduct>;
    itemDetails: EventEmitter<IProduct>;
    selectCurrency: CURRENCIES_SYMBOL;
    discountPercentage: string;
    constructor();
    ngOnInit(): void;
    addToCart(product: IProduct): void;
    showDetails(product: IProduct): void;
}
